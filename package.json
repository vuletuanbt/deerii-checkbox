{
  "name": "@deerii/component",
  "version": "0.7.1",
  "private": false,
  "type": "module",
  "main": "./dist/component.cjs",
  "module": "./dist/component.js",
  "types": "./dist/components/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/component.js",
      "require": "./dist/component.cjs"
    },
    "./helper": {
      "types": "./dist/helpers/index.d.ts",
      "import": "./dist/helper.js",
      "require": "./dist/helper.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "files": [
    "dist",
    "src/components"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "lint": "eslint --fix src",
    "format": "prettier . --write",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.4.37",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@types/node": "^22.5.1",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.28.0",
    "global": "^4.4.0",
    "path": "^0.12.7",
    "prettier": "^3.3.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.4.0",
    "vite": "^5.4.1",
    "vite-plugin-css": "^1.0.4",
    "vite-plugin-dts": "^4.0.3",
    "vue-tsc": "^2.0.29"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuletuanbt/deerii-component.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "component",
    "deerii",
    "material",
    "material-ui",
    "vue3",
    "vue",
    "vuejs"
  ]
}
